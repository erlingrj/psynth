target C {
    files: ["freq.c","midi.h","common.h", "freq.h"],
    cmake-include: "include.cmake",
    build-type: Debug,
    // fast: true,
    // tracing: true,
}

import SoundOutput from "SoundOutput.lf"
import Synth from "Synth.lf"
import UserInterface from "UserInterface.lf"

main reactor {
    synth = new Synth()
    out = new SoundOutput()
    ui = new UserInterface()


    ui.midi_event -> synth.midi_event
    synth.samples -> out.samples
    synth.samples -> ui.samples

}
